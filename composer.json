{
  "name": "leantony/laravel_settings",
  "description": "Dynamically load laravel config settings from config files into a database",
  "license": "MIT",
  "authors": [
    {
      "name": "leantony",
      "email": "chachaantony@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "~5.1",
    "illuminate/database": "~5.1",
    "illuminate/filesystem": "~5.1",
    "illuminate/cache": "~5.1",
    "illuminate/log": "~5.1",
    "illuminate/console": "~5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "classmap": [
    "src/migrations"
  ],
  "autoload": {
    "psr-4": {
      "Leantony\\Settings\\": "src/",
      "Leantony\\Settings\\Commands\\": "src/commands",
      "Leantony\\Settings\\Models\\": "src/models",
      "Leantony\\Settings\\Providers\\": "src/providers"
    }
  }
}
